class Main {
    // entry point
    function void main(){
        var Game game;

        do MoveableObj.init();
        do Game.init();
        do Ghost.init();
        do Pacman.init();
        do Map.init();
        
        let game = Game.new();
        do game.run();
        
        do game.dispose();
        do MoveableObj.release();
        do Ghost.release();
        do Pacman.release();
        do Map.release();
        return;
    }
}